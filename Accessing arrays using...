#include <iostream>
using namespace std;


int main ()
{
int arr [6];
cout << "Enter 6 digits:";

//intaking elements
for (int i=0; i <6; i++){
    cin >> arr[i];
}

//defining a pointer
int * point = arr;

//output 
cout << "You have entered:\n";
for (int i=0; i <6; i++) {
    cout << *(point+i)<<endl;
}
return 0;
}
